  "scripts": {
    "start": "set PORT=3333 && react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "indent": [
        "warn",
        2
      ],
      "semi": [
        "warn",
        "always"
      ],
      "quotes": [
        "warn",
        "single"
      ],
      "jsx-quotes": [
        "warn",
        "prefer-double"
      ],
      "no-multiple-empty-lines": [
        "warn",
        {
          "max": 1,
          "maxBOF": 1,
          "maxEOF": 1
        }
      ],
      "object-curly-newline": [
        "warn",
        {
          "consistent": true
        }
      ],
      "semi-spacing": [
        "warn",
        {
          "before": false,
          "after": true
        }
      ],
      "comma-spacing": [
        "warn",
        {
          "before": false,
          "after": true
        }
      ],
      "no-var": "warn",
      "object-curly-spacing": [
        "warn",
        "always"
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^12.8.3",
    "redux-logger": "^3.0.6",
    "web-vitals": "^1.1.0"
  }
}
